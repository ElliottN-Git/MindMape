<div class="row">
    {{#if message}}
        <p>{{message}}</p>
    {{/if}}
    <div class="page-header">
        <h1>{{fname}}'s Profile</h1>
    </div>

    <table class="table table-striped">
        
        <tbody>
            <tr>
                <th>First Name</th>
                <td>{{fname}}</td>
            </tr>
            <tr>
                <th>Last Name</th>
                <td>{{lname}}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{email}}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{gender}}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{country}}</td>
            </tr>
            <tr>
                <th>Phone number</th>
                <td>{{phoneNum}}</td>
            </tr>
        </tbody>
    </table>
</div>