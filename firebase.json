{
  "hosting": {
    "public": "./public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "/js"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "mindMAPE"
      }
    ]
  },
  "functions": [
    {
      "source": "./",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "views"
      ],
      "runtime": "nodejs18"
    }
  ]
}